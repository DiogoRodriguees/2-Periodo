{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Iniciar",
      "type": "cppdbg",
      "request": "attach",
      "program": "${workspaceFolder}/a.exe",
      "processId": "${command:pickProcess}",
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\MinGW\\bin\\gdb.exe",
      "setupCommands": [
        {
          "description": "Habilitar a reformatação automática para gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Definir Tipo de Desmontagem como Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "c:/Users/Windows/Documents/Faculdade/2° Periodo/Algoritimos e Estrutura de Dados/Listas/lista/lista-refatorada2/lista-encadeada-sentinela",
      "program": "c:/Users/Windows/Documents/Faculdade/2° Periodo/Algoritimos e Estrutura de Dados/Listas/lista/lista-refatorada2/lista-encadeada-sentinela/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}